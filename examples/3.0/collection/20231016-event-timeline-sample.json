[
    {
        "uri": "https://iptc.org/example/uri/1",
        "title": "Athlete uses medicine containing prohibited substance",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "The Norwegian team doctor bought a medicine  containing the antibiotic neomycin. The doctor also noticed that the prohibited substance was an ingredient, but did not identify it as a Prohibited Substance."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-09-03",
                "estimatedEndDate": "2016-09-03"
            }
        },
        "events": {
            "id": "1",
            "name": "anti-doping rule-violation",
            "literal": "ADRV"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/2",
        "title": "Athlete is tested Out-of-Competition",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "Athlete underwent an unannounced doping control test conducted by the Norwegian Anti-Doping Committee. She provided blood and urine samples."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-09-16",
                "estimatedEndDate": "2016-09-16"
            }
        },
        "events": {
            "id": "2",
            "name": "Out-of-Competition Test",
            "literal": "OOC"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/3",
        "title": "The sample returns a positive result",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "A sample was tested by the Norwegian Doping Control Laboratory. The test report submitted on 30 September 2016 noted the presence of a Clostebol metabolite at an estimated concentration of 13 ng/mL"
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-09-30",
                "estimatedEndDate": "2016-09-30"
            }
        },
        "events": {
            "id": "3",
            "name": "Test report",
            "literal": "TR"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/4",
        "title": "B-sample was analyzed and confirms the A-sample",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "At the request of Anti-Doping Norway, the B sample was analyzed on 6 October 2016. This analysis confirmed the A sample results."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-10-06",
                "estimatedEndDate": "2016-10-06"
            }
        },
        "events": {
            "id": "4",
            "name": "B-sample analysis",
            "literal": "BSA"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/5",
        "title": "Athlete is provisionally suspended",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "The Prosecution Committee for Anti-Doping Norway imposed a twomonth provisional suspension on Ms Athlete commencing that same date, until 18 December 2016."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-10-18",
                "estimatedEndDate": "2016-10-18"
            }
        },
        "events": {
            "id": "5",
            "name": "Provisional Suspension",
            "literal": "PSU"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/6",
        "title": "Provisionally suspension prolonged with two months",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "the Prosecution Committee suspended Ms Athlete for another two months, until 19 February 2017, pending a final judgment."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2016-12-12",
                "estimatedEndDate": "2016-12-12"

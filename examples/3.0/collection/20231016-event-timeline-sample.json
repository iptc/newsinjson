    },
    {
        
        "uri": "https://iptc.org/example/uri/9",
        "title": "The Adjudication Committee of Anti-Doping Norway imposses decision",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "On 10 February 2017, the Adjudication Committee issued its Decision. It noted that the facts of the case were undisputed by the parties involved. The Decision focused on the degree of fault to be attributed to Ms Athlete, and the proportionate sanction. The Adjudication Committee suspended Ms Athlete for thirteen months, commencing 18 October 2016."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2017-02-10",
                "estimatedEndDate": "2017-02-10"
            }
        },
        "events": {
            "id": "9",
            "name": "Decision",
            "literal": "DEC"
        }
    },

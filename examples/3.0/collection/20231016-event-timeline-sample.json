    {
        
        "uri": "https://iptc.org/example/uri/7",
        "title": "Public hearing at the Adjudication Committee of Anti-Doping Norway - day 1",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "On 25 and 26 January 2017, The Adjudication Committee of Anti-Doping Norway (the “Adjudication Committee”) conducted a public hearing in Oslo. It heard testimony from numerous witnesses including Ms Athlete herself and Dr Bendiksen. Anti-Doping Norway requested a sanction of fourteen months to be imposed on Ms Athlete."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2017-01-25",
                "estimatedEndDate": "2017-01-25"
            }
        },
        "events": {
            "id": "7",
            "name": "Doping Hearing",
            "literal": "HEA"
        }
    },
    {
        
        "uri": "https://iptc.org/example/uri/8",
        "title": "Public hearing at the Adjudication Committee of Anti-Doping Norway - day 2",
        "descriptions": [
            {
                "role": "text",
                "contentType": "text/plain",
                "value": "On 25 and 26 January 2017, The Adjudication Committee of Anti-Doping Norway (the “Adjudication Committee”) conducted a public hearing in Oslo. It heard testimony from numerous witnesses including Ms Athlete herself and Dr Bendiksen. Anti-Doping Norway requested a sanction of fourteen months to be imposed on Ms Athlete."
            }
        ],
        "eventDetails": {
            "dates": {
                "estimatedStartDate": "2017-01-26",
                "estimatedEndDate": "2017-01-26"
            }
        },
        "events": {
